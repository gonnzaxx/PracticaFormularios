<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practica1">
        <activity
            android:name=".Datos"
            android:exported="false"
            android:label="@string/title_activity_datos"
            android:theme="@style/Theme.Practica1" />
        <activity
            android:name=".Contactos"
            android:exported="false"
            android:label="@string/title_activity_contactos"
            android:theme="@style/Theme.Practica1" />
        <activity
            android:name=".Inicio"
            android:exported="true"
            android:label="@string/title_activity_inicio"
            android:theme="@style/Theme.Practica1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Registro"
            android:exported="false"
            android:label="@string/title_activity_formulario"
            android:theme="@style/Theme.Practica1" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Practica1"></activity>
    </application>

</manifest>